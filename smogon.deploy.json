{
  smogonId: [
    {
      mount: "forumsprites",
        in: [
          "build/gen6-minisprites-padded/canonical/pokemon",
          "build/gen6-minisprites-padded/noncanonical/pokemon"
        ]
    },
    {
      mount: "fbsprites/xy",
        in: [
          "build/smogon/fbsprites/xy"
        ]
    },
    {
      mount: "twittersprites/xy",
        in: [
          "build/smogon/twittersprites/xy"
        ]
    }
  ]
}
